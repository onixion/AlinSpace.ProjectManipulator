<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net5.0;net6.0</TargetFrameworks>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>1.1.0</Version>
    <Authors>Alin Andersen</Authors>
    <PackageProjectUrl>https://github.com/onixion/AlinSpace.ProjectManipulator</PackageProjectUrl>
    <RepositoryUrl>https://github.com/onixion/AlinSpace.ProjectManipulator</RepositoryUrl>
    <Description>.NET project file manipulator.</Description>
    <Copyright>Â© 2022 Alin Andersen</Copyright>
    <PackageIcon>Icon.png</PackageIcon>
    <RepositoryType>https://github.com/onixion/AlinSpace.ProjectManipulator</RepositoryType>
    <PackageTags>dotnet; net; project; manipulator; manipulation; projects; solution</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="Icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </EmbeddedResource>
  </ItemGroup>

</Project>
